{
  "name": "packemon-native-dynamic-import",
  "version": "1.0.0",
  "description": "",
  "repository": "azu/packemon-native-dynamic-import",
  "scripts": {
    "build": "packemon pack --addEngines --addExports --declaration --declarationConfig tsconfig.json",
    "tsc:build": "tsc"
  },
  "packemon": [
    {
      "inputs": {
        "index": "./src/index.ts"
      },
      "platform": "node",
      "format": "cjs"
    },
    {
      "inputs": {
        "node": "./src/index.ts"
      },
      "platform": "node",
      "format": "mjs"
    }
  ],
  "author": "azu",
  "license": "MIT",
  "devDependencies": {
    "packemon": "^2.3.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "pkg-dir": "^6.0.1"
  }
}
